{
    "manifest_version": 2,
    "name": "Gestionnaire de Workspaces",
    "version": "1.0",
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_title": "Gestionnaire de Workspaces",
      "default_popup": "popup.html"
    },
    "permissions": ["tabs", "storage"],
    "web_accessible_resources": ["tab.html"]
  }
  